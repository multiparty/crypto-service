    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "jsdoc": "^3.5.5",
    "libsodium-wrappers-sumo": "^0.7.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.8",
    "ts-node": "^6.1.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
